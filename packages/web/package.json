    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "@webcomponents/custom-elements": "^1.4.3",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
    "eslint": "^7.26.0",
    "firebase-tools": "^9.11.0",
